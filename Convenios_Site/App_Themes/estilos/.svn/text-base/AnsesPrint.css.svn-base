@page
{
	size: auto; 
	margin: 0cm;

}

body
{
	margin-left: 0;
	margin-top: 0px;
	font-family: Verdana, Arial;
	font-size: 7pt;
	color: #000000;	
}

@media print
{
   #contenedor
   {width:100%}
}

@media screen
{
   #contenedor
	{
		width: 750px;
		border-left: solid 1px #c0c0c0;
		border-right: solid 1px #c0c0c0;
		border-bottom: solid 1px #c0c0c0;
		padding: 5px 10px 5px 10px;
		height:100%;
	}
	body
	{
		text-align:center;
		
	}
	
}

.Cabecera
{
	top: 0;
	left: 0;
	width: 100%;
	font-family: Verdana, Arial;
	font-size: 7pt;
	color: #000000;	
}

.saltopagina 
{ 
page-break-after: always; 
}


.grilla
{
	page-break-after: avoid;
	background-color:#000;
	border:solid 0px #000;
	color:#000;	

}

.grillalernateitem
{
	border:solid 1px #000;
	background-color:#fff;
}

.grillabody
{
	border:solid 1px #000;
	color:#000;
	background-color:#fff;
}
.grillaitem
{
	border:solid 1px #000;
	color:#000;
	background-color:#fff;
}
.cc
{
	page-break-after: auto;
	}
.grillahead
{
	display: table-header-group;
	background-color:#fff;
	border-style:solid 	;
	color:#000
}
.grillaPie
{
	background-color:#fff;
	border-style:solid 	;
	color:#000;
}

.titulo
{
	font-size:9pt;
	font-weight:bold;	
}

.bodesCeldas
{
	text-align: left; 
	border-top: solid 1px #000; 
	border-left: solid 1px #000
}

.bodesCeldasCentradas
{
	text-align: Center; 
	border-top: solid 1px #000; 
	border-left: solid 1px #000
}
 .ser td
{
	border-collapse:collapse;
	border-style:solid;
	border-color:#d3d3d3;
	border-width:inherit;
	/*border: 1px solid #a9a9a9;	*/
	
}

