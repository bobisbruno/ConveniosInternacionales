.solapa
{
	text-align: center;
	text-decoration: none;
	margin: 1;
	float: left;
	display: block;
	background: url(  "Estilos/Imagenes/tabright5.gif" ) right top;
	padding: 6px 6px 4px 6px;
	color: #3D7EC7;
	height: 20px;
	font-weight: bold;
	font-size: 8pt;
	width: 85px;
	font-family: Verdana, 'Microsoft Sans Serif';
	
}
      
.solapa:hover
{
	font-family: Verdana, 'Microsoft Sans Serif';
	background-position: 100% -49px;
	font-size: 8pt;
	width: 85px;
	color: #0B3D91;
}


.solapa_activo 
{
	background: url(  "Estilos/Imagenes/tabright5.gif" ) right top;
	background-position: 100% -49px; 
	color: #0b3d91; 
	text-align: center;
